set default=0>0
set timeout=15

echo "Ladataan    / Laddas   "
insmod vbe
clear
echo "Ladataan.   / Laddas.  "
insmod efi_gop
clear
echo "Ladataan..  / Laddas.. "
insmod efi_uga
clear
echo "Ladataan... / Laddas..."

if loadfont ${prefix}/fonts/unicode.pf2
then
        insmod gfxterm
        set gfxmode=auto
        set gfxpayload=keep
        terminal_output gfxterm
fi

set color_normal=white/black
set color_highlight=cyan/black

insmod gettext
set lang=fi
insmod progress

if cpuid -l ; then

submenu "Koetilapalvelin / Provlokalens server" {

  menuentry "PC Koetilapalvelin / Provlokalens server" {
    linux           @LINUX@ @APPEND_LIVE@ quiet splash ip=eth0:10.10.0.1:255.255.0.0 hostname=ktp1.local persistence
    initrd          @INITRD@
  }

  menuentry "Mac Koetilapalvelin / Provlokalens server" {
    linux           @LINUX@ @APPEND_LIVE@ quiet nomodeset xforcevesa ip=eth0:10.10.0.1:255.255.0.0 hostname=ktp1.local persistence
    initrd          @INITRD@
  }
}

submenu "Lis√§valvontakonsoli / Ytterligare kontrolkonsol" {

  menuentry "PC Valvontakonsoli / Kontrollkonsol" {
    linux           @LINUX@ @APPEND_LIVE@ quiet splash remote-supo
    initrd          @INITRD@
  }

  menuentry "Mac Valvontakonsoli / Kontrollkonsol" {
    linux           @LINUX@ @APPEND_LIVE@ quiet nomodeset xforcevesa remote-supo
    initrd          @INITRD@
  }

}

else
echo "Koetilapalvelin vaatii 64-bittisen prosessorin / Provlokalens server kraver en 64 bits processor"
fi